<!DOCTYPE html>
<html>
<head>
<style>
table {
  font-family: arial, sans-serif;
  border-collapse: collapse;
  width: 100%;
}

td, th {
  border: 1px solid #dddddd;
  text-align: left;
  padding: 8px;
}

tr:nth-child(even) {
  background-color: #dddddd;
}
</style>
</head>
<body>
    
    <h2>HTML Table</h2>
    
    <table>
        <tr>
            <th>Company</th>
            <th>Contact</th>
            <th>Country</th>
            <th>Country</th>
        </tr>
        <% for(var i=0; i<data.length; i++) {%>
        <tr>
            <td> <% data[i].username %> </td>
            <td> <% data[i]._id %> </td>
            <td> <% data[i].login_name %> </td>
            <td> <% data[i].groupname %> </td>
    
  </tr>
   } %>
  
</table>

</body>
</html>

